{
	"name": "tomzx/personal-automation",
	"description": "Personal automation",
	"type": "library",
	"keywords": [
		"personal",
		"automation"
	],
	"homepage": "https://github.com/tomzx/personal-automation",
	"license": "MIT",
	"authors": [
		{
			"name": "Tom Rochette",
			"email": "tom@tomrochette.com",
			"homepage": "http://www.tomrochette.com"
		}
	],
	"support": {
		"issues": "https://github.com/tomzx/personal-automation/issues"
	},
	"require": {
		"php": ">=7.1.0",

		"symfony/dom-crawler": "^7",
		"symfony/css-selector": "^7"
	},
	"require-dev": {
		"phpunit/phpunit": "^11"
	},
	"autoload": {
		"psr-4": {
			"MyNamespace\\": "src/MyNamespace"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"MyNamespace\\Tests\\": "tests/MyNamespace"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.1-dev"
		}
	}
}
